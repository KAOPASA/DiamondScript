_G.Hwid = game:GetService("RbxAnalyticsService"):GetClientId()
local Check = "https://x2goldd.000webhostapp.com/Server.php?Key=".._G.Key.."-".._G.Hwid
local HttpService = game:GetService("HttpService");

function SendMessage(Webhook,massage) 
   local API = "http://buritoman69.glitch.me/webhook";
   local Body = {
       ['Key'] = tostring("applesaregood"),
       ['Message'] = massage,
       ['Name'] = "Hwid bot",
       ['Webhook'] = Webhook    
   }
   Body = HttpService:JSONEncode(Body);
   local Data = game:HttpPost(API, Body, false, "application/json")
   return Data or nil;
end

if game:HttpGet(Check) == "WHITELIST!" then
   print("Whitelist !!")
   loadstring(game:HttpGet("https://raw.githubusercontent.com/KAOPASA/DiamondScript/main/"..game.PlaceId))();
elseif game:HttpGet(Check) == "Invaid Key!" then
   print("Invaid Key")
elseif game:HttpGet(Check) == "Invaid Key!KeyWhitlist" then
   SendMessage("https://discord.com/api/webhooks/940893042292965397/vSTbR4lO9RSq_Q-N6iTnxLmYqxBjBbpBQSNhteTqmYwHRzsP-xrQptZfx3tvmk3KESzC","<@&937881271693111316>".."KeyWhitlist".._G.Key.."  "..game:GetService("RbxAnalyticsService"):GetClientId())
   game.Players.localPlayer:Kick("KeyWhitlist pls wait Rejoin")
   game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").localPlayer)
end

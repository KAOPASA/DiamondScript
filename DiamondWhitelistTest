_G.Key = ""
_G.Hwid = game:GetService("RbxAnalyticsService"):GetClientId()

if game:HttpGet("https://x2goldd.000webhostapp.com/Server.php?Key=".._G.Key"&HWID=".._G.Hwid) == "WHITELIST !" then
   print("Whitelist !!")
elseif game:HttpGet("https://x2goldd.000webhostapp.com/Server.php?Key=".._G.Key"&HWID=".._G.Hwid) ~= "WHITELIST !" then
   game.Players.localPlayer:Kick("Wrong Hwid or Key !!")
end
